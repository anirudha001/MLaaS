package org.laas.test.datagenerator;

import org.apache.log4j.Logger;

public class Log4JDataGenerator {

	
	Logger log;
	int noOfRows;
	
	public Log4JDataGenerator() {
		log = Logger.getRootLogger();
		noOfRows = 50;
	}
	
	public void generateData() {
		
		for (int loop = 0; loop < noOfRows; loop++) {
			
			log.debug("" + loop);
			log.info("" + loop);
			
		}

	}
}
